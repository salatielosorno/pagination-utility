{
	"name": "pagination-utility",
	"version": "0.1.1",
	"description": "Utility to implement pagination functionality",
	"main": "dist/index.js",
	"scripts": {
		"prebuild": "rimraf ./dist",
		"build": "tsc",
		"test": "jest",
		"lint": "eslint . --ext .ts",
		"lint-and-fix": "eslint . --ext .ts --fix",
		"format": "prettier --config .prettierrc \"src/**/*.ts\" --write"
